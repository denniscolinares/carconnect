{
	"restartable" : "rs",
	"verbose" : true,
	"execMap": {
		"js": "node --harmony --unhandled-rejections=strict --max-old-space-size=1024 --trace-warnings"
	},
	"ignore" : [
		"node_modules/**/node_modules",
		".next",
		".git"
	],
	"watch" : [
		"server/",
		"library/",
		"server.js"
	],
	"env": {
		"NODE_ENV": "development"
	},
	"ext" : "js,scss,css,json"
}
